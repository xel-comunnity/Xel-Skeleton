{
    "name": "xel/skeleton",
    "description": "Framework Skeleton for xel framework",
    "type": "project",
    "authors": [
        {
            "name": "Bxel07",
            "email": "yogipratama.ut@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xel\\Devise\\": "devise/",
            "Xel\\Setup\\" : "setup/",
            "Xel\\Migration\\": "migration/",
            "Xel\\MigrationSeeder\\": "migration_seeder/"
        },
        "files": [
            "setup/Dock/DockEntry.php",
            "devise/AppClassBinder/ServiceRegister.php"
        ]
    },
    "require": {
        "php": ">=8.2",
        "xel/asycn": "dev-main",
        "xel/logger": "dev-main",
        "symfony/console": "7.1.x-dev",
        "vlucas/phpdotenv": "^5.6@dev",
        "ext-pdo": "*",
        "firebase/php-jwt": "dev-main",
        "swoole/ide-helper": "@dev",
        "ezyang/htmlpurifier": "dev-master"
    },
    "minimum-stability": "dev",
    "scripts": {
        "watcher-css": "./tailwindcss -i ./public/css/input.css -o ./public/css/output.css --watch"
    }
}
